{
  "verbose": true,
  "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
  "testRegex": "specs/.*[\\.\\-][Ss]pec\\.js$",
  "coverageDirectory": "<rootDir>/coverage",
  "coveragePathIgnorePatterns": ["/node_modules/", "/specs/"],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}